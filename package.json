{
  "name": "@uniglot/wont-let-you-see",
  "version": "0.2.0",
  "description": "OpenCode plugin that masks sensitive cloud infrastructure data (AWS, Kubernetes) from LLMs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "type": "module",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "opencode",
    "plugin",
    "security",
    "aws",
    "kubernetes",
    "k8s",
    "masking",
    "privacy"
  ],
  "author": "Yuneui Jeong (contact@uniglot.dev)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uniglot/wont-let-you-see.git"
  },
  "scripts": {
    "test": "vitest",
    "build": "bun build src/index.ts --outdir=dist --target=node --format=esm",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "devDependencies": {
    "@types/bun": "latest",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@opencode-ai/plugin": "^1.1.35"
  }
}
